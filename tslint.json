{
    "defaultSeverity": "error",
    "extends": [
      "tslint:recommended"
    ],
    "rules": {
      "semicolon": [false],
      "quotemark": [true, "single"],
      "interface-over-type-literal": [false],
      "trailing-comma": [true, {
        "multiline": {
          "objects": "always",
          "arrays": "always",
          "functions": "never",
          "typeLiterals": "never"
        },
        "esSpecCompliant": true
      }],
      "ordered-imports": [false],
      "object-literal-sort-keys": [false],
      "align": [false]
    }
  }